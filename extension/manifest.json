{
  "name": "saipuproxy",
  "version": "0.1",
  "manifest_version": 2,
  "icons" :{
    "16" :"images/logo.png",
    "48" : "images/logo48.png",
    "96" : "images/logo96.png",
    "128" : "images/logo128.png"
},
"description": "Adds a red border to all webpages matching mozilla.org.",
  "content_scripts": [
    {
        "matches": ["*://localhost:*/*"],
        "js": ["fd.js"]
    }
  ],
  "applications": {
      "gecko": {
        "id": "borderify@example.com"
      }
    }
}
