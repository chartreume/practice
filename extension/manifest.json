{
  "name": "saipuproxy",
  "version": "0.0.0.43",
  "manifest_version": 2,
  "icons" :{
    "16" :"images/logo16_b.png",
    "48" : "images/logo48_b.png",
    "128" : "images/logo128_b.png"
},
  "content_scripts": [
    {
        "css" : [],
        "matches": ["*://*.rpo.org/*"],
        "js": ["fd.js"]
    }
  ],

  "background" : {
         "scripts" : [
             "bg.js"
         ]
     },


  "permissions": ["*://*/","unlimitedStorage","webRequest", "webRequestBlocking"],
  "web_accessible_resources":["images/*","js/*"]
}
